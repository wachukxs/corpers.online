{
  "name": "corpers-online",
  "version": "1.0.2",
  "private": true,
  "description": "for corpers by corpers",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified... yet\" && exit 1",
    "dev:true": "DEV=true nodemon --exec 'node ./swagger.js && node --trace-warnings --unhandled-rejections=warn ./index.js'",
    "dev": "nodemon --exec 'node ./swagger.js && node --trace-warnings --unhandled-rejections=warn ./index.js'",
    "start:trace-io": "nodemon --trace-sync-io --trace-warnings --unhandled-rejections=warn ./index.js",
    "start": "node ./swagger.js && node --trace-warnings --unhandled-rejections=warn ./index.js",
    "restart": "yarn run seed:down:all && yarn run migrate:down:all && yarn run migrate:up && yarn run seed:up:all && yarn run dev",
    "soft-restart": "yarn run migrate:up && yarn run seed:up:all && yarn run dev",
    "swagger-autogen": "node ./swagger.js",
    "seed:up": "npx sequelize-cli db:seed",
    "seed:up:all": "npx sequelize-cli db:seed:all",
    "seed:down": "npx sequelize-cli db:seed:undo",
    "seed:down:all": "npx sequelize-cli db:seed:undo:all",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:down": "npx sequelize-cli db:migrate:undo",
    "migrate:down:all": "npx sequelize-cli db:migrate:undo:all"
  },
  "engines": {
    "yarn": "1.x",
    "node": "18.x"
  },
  "keywords": [
    "corpers",
    "corp members",
    "Nigeria",
    "information",
    "transportation",
    "PPA",
    "SPA",
    "know",
    "places"
  ],
  "author": "@wachukxs, Mogaba Juliet, Chukwugbogum Blessing",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wachukxs/corpers.online.git"
  },
  "license": "ISC",
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.0",
    "basic-ftp": "^5.0.4",
    "bcrypt": "5.0.0",
    "body-parser": "^1.19.0",
    "busboy": "^1.6.0",
    "connect-session-knex": "^2.1.0",
    "connect-session-sequelize": "^7.1.1",
    "cookie-parser": "1.4.5",
    "cookieparser": "^0.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "ejs-lint": "^1.2.0",
    "express": "^4.17.1",
    "express-mysql-session": "2.1.4",
    "express-session": "^1.17.0",
    "googleapis": "^39.2.0",
    "helmet": "3.22.0",
    "joi": "^17.8.4",
    "jsonwebtoken": "8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "mysql2": "^2.2.5",
    "nodemailer": "^6.4.2",
    "nodemon": "2.0.4",
    "npx": "^10.2.2",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.3",
    "pug": "^2.0.4",
    "sequelize": "^6.6.2",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^4.7.4",
    "sqlite3": "^5.1.7",
    "swagger-autogen": "^2.22.0",
    "swagger-ui-express": "^4.6.0",
    "web-push": "^3.4.5"
  },
  "eslintConfig": {},
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "migrations/*",
      "old models/*",
      "test/*",
      "js/*",
      "index2.js",
      "/sw.js",
      "/serviceWorkerNotification.js",
      "*.json",
      "node_modules/**/node_modules"
    ],
    "ext": ".js"
  },
  "devDependencies": {
    "@types/socket.io": "^3.0.2"
  }
}
